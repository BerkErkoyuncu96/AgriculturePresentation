@model EntityLayer.Concrete.Admin
@{
    ViewData["Title"] = "addAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Yeni Admin Ekleyin</h4>
            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label for="exampleInputTitle">Kullanıcı Adı</label>
                    <input type="text" class="form-control" id="exampleInputUsername" placeholder="Kullanıcı Adı" asp-for="Username">
                </div>
                <div class="form-group">
                    <label for="exampleInputTitle">Şifre</label>
                    <input type="password" class="form-control" id="exampleInputPassword" placeholder="Şifre" asp-for="Password">
                </div>
                <button type="submit" class="btn btn-outline-primary">Kaydet</button>
            </form>
        </div>
    </div>
</div>



