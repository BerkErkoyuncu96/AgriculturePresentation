@using EntityLayer.Concrete
@model List<Adress>


<h3 class="tittle">Bize Ulaşın</h3>
<p class="heading-btm-w3ls">Aşağıda bulunan adresleri kullanarak bize mail atabilir veya bize telefonla arayarak ulaşabilirsiniz.</p>
@foreach(var item in Model)
{
<div class="col-md-3 contact-left">
	<h4>Konum Bilgilerimiz</h4>
	<p>@item.Description1,</p>
	<p>@item.Description3,</p>
	<p>@item.Description2</p>
	<h4>Telefon Numaramız</h4>
	<p>@item.Description4</p>

</div>
}